<script>

  const urlParams = new URLSearchParams(window.location.search);
  const tokenUrl = urlParams.get('session-id');

  const tokenLocal = window.localStorage.getItem("session-id");

  const token = tokenUrl || tokenLocal;

  if (typeof token === 'string' && token !== 'null') {
    window.localStorage.setItem("session-id", token);
  }

  const signedIn = typeof token === "string";
  if (!signedIn) {
    window.location.replace("/sign-in");
  }

  function logout() {
    window.localStorage.removeItem("session-id");
    window.location.replace("/sign-in");
  }

</script>
