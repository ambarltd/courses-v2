{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "isActive": {
        "type": "boolean"
      },
      "paymentCycle": {
        "type": "string"
      },
      "annualFeeInCents": {
        "type": "integer"
      },
      "creditLimitInCents": {
        "type": "integer"
      },
      "reward": {
        "type": "string"
      }
    },
    "required": [
      "id",
      "name",
      "isActive",
      "paymentCycle",
      "annualFeeInCents",
      "creditLimitInCents",
      "reward"
    ]
  }
}
